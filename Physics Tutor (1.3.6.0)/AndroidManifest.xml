<?xml version="1.0" encoding="utf-8"?>
<manifest
xmlns:android="http://schemas.android.com/apk/res/android"
package="com.musicslayer.physicstutor"
android:versionName="1.3.6"
android:versionCode="12">

    <application
    android:label="@string/app_name"
    android:icon="@drawable/icon">

        <activity android:name="PhysicsIntroActivity">
            <intent-filter><action android:name="android.intent.action.MAIN"/><category android:name="android.intent.category.LAUNCHER"/></intent-filter>
        </activity>
        <activity android:name="PhysicsNagActivity" android:configChanges="keyboardHidden|orientation"/>
        <activity android:name="PhysicsConstantsActivity" android:configChanges="keyboardHidden|orientation"/>
        <activity android:name="PhysicsHelpActivity" android:configChanges="keyboardHidden|orientation"/>
        <activity android:name="PhysicsProblemsActivity" android:configChanges="keyboardHidden|orientation"/>
        <activity android:name="PhysicsSettingsActivity" android:configChanges="keyboardHidden|orientation"/>
        <activity android:name="PhysicsSolverActivity" android:configChanges="keyboardHidden|orientation"/>
        <activity android:name="PhysicsInputActivity" android:configChanges="keyboardHidden|orientation"/>
        <activity android:name="PhysicsInfoActivity" android:configChanges="keyboardHidden|orientation"/>
        <activity android:name="com.google.ads.AdActivity" android:configChanges="keyboard|keyboardHidden|orientation"/>

        <meta-data android:name="ADMOB_PUBLISHER_ID" android:value="a14debddae9593b"/>
        <meta-data android:name="ADMOB_ALLOW_LOCATION_FOR_ADS" android:value="false"/>
    </application>

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>

    <uses-sdk android:minSdkVersion="3"/>
    <uses-sdk android:targetSdkVersion="5"/>
</manifest> 